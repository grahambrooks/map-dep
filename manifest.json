{
  "manifest_version": "0.3",
  "name": "mcp-ping",
  "display_name": "MCP Ping Server",
  "version": "0.1.0",
  "description": "A simple MCP server with a ping tool that responds with pong",
  "author": {
    "name": "Graham Brooks",
    "url": "https://github.com/grahambrooks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grahambrooks/mcp-dep"
  },
  "license": "MIT",
  "keywords": ["mcp", "ping", "example"],
  "server": {
    "type": "binary",
    "entry_point": "mcp-ping",
    "mcp_config": {
      "command": "${__dirname}/mcp-ping",
      "args": []
    }
  },
  "tools": [
    {
      "name": "ping",
      "description": "A simple ping tool that responds with pong"
    }
  ],
  "compatibility": {
    "platforms": ["darwin", "win32", "linux"]
  }
}
